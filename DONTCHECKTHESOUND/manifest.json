{
  "manifest_version": 3,
  "name": "DONT CLICK THE SOUND",
  "version": "1.0",
  "description": "Such a W Script by me",
  "permissions": ["scripting", "tabs"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "run_at": "document_idle"
    }
  ],
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}
